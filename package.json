{
  "name": "linter-bailey",
  "main": "./lib/init",
  "linter-package": true,
  "version": "0.2.2",
  "private": true,
  "description": "Atom linter plugin for bailey.js",
  "repository": "https://github.com/AtomLinter/linter-bailey",
  "license": "MIT",
  "engines": {
    "atom": ">0.177.0"
  },
  "dependencies": {
    "bailey": "*"
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "semanticCommits": true,
    "rangeStrategy": "pin",
    "packageRules": [
      {
        "packagePatterns": [
          "^eslint"
        ],
        "groupName": "ESLint packages"
      }
    ]
  }
}
